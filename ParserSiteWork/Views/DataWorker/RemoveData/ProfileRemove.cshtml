@model List<DeleteProfileModel>

<h3>Удаление компетенции</h3>
<form asp-action="RemoveProfile" asp-controller="DataRemover">
	<p>
		@for (int i = 0; i < Model.Count; i++)
		{
			<div>
				<p>@Model[i].Profile?.ProTitle</p>
				<p>@Model[i].Profile?.Subject</p>
				<p>@Model[i].Profile?.ProYear</p>
				<p>@Model[i].Profile?.ProAdminissionYear</p>
			</div>

			@Html.CheckBoxFor(m => Model[i].ToDelete)
		}
		<input type="submit" value="Отправить"/>
	</p>
</form>