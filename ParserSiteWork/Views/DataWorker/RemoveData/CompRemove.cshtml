@model List<DeleteCompModel>

<h3>Удаление компетенции</h3>
<form asp-action="RemoveCompetence" asp-controller="DataRemover">
	<p>
		@for (int i = 0; i < Model.Count; i++)
		{
			<div>
				<p>@Model[i].Competence?.CompNumber</p>
				<p>@Model[i].Competence?.CompAnnotation</p>
			</div>

			@Html.CheckBoxFor(m => Model[i].ToDelete)
		}
		<input type="submit" value="Отправить"/>
	</p>
</form>