@{
    ViewBag.question_count = 0;
}
@model List<Question>

<h2>Вопросы</h2>

    <table>
        <tr>
            <th>Номер вопроса</th>
            <th>Описание</th>
            <th>Компетенция</th>
        </tr>

        @for (int i = 0; i < Model.Count; i++)
        {
            Question question = Model[i];

            string q_num = i.ToString();
            <tr>
                <td>
                    @question.Number
                </td>
                <td>
                    @question.Description
                </td>
                <td>
                    @question.Competention.Name
                </td>
            </tr>
        }

    </table>
<br></br>